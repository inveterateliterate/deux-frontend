{
  "name": "deux-client",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "setup": "node scripts/setup.js",
    "build": "yarn run clean && webpack --config ./config/webpack.config.production",
    "build:development": "webpack-dev-server --config ./config/webpack.config.development.js",
    "clean": "rm -rf build/*",
    "lint": "eslint src/ --max-warnings=0",
    "start": "yarn install && yarn run build:development",
    "server": "node server.js",
    "load-env": "node scripts/load-env.js",
    "test": "yarn install && node scripts/test-suite.js",
    "test:unit": "jest --coverage",
    "test:integration": "cypress run",
    "seed:test": "echo 'No script specified, seed skipped'",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "heroku-prebuild": "yarn remove node-sass",
    "heroku-postbuild": "yarn run build",
    "generate-bundle-stats": "yarn run clean && webpack --config config/webpack.config.production.js --profile --json > .webpack-stats.json",
    "bundle-analyzer-server": "webpack-bundle-analyzer .webpack-stats.json build/",
    "analyze-bundle": "echo 'Building bundle...' && yarn run generate-bundsle-stats && yarn run bundle-analyzer-server"
  },
  "dependencies": {
    "@launchpadlab/lp-components": "^3.0.0",
    "@launchpadlab/lp-form": "^2.2.1",
    "@launchpadlab/lp-hoc": "^2.0.0",
    "@launchpadlab/lp-redux-api": "^5.0.4",
    "@launchpadlab/lp-redux-utils": "^1.1.2",
    "@launchpadlab/lp-requests": "^3.1.0",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-flash": "^1.0.1",
    "redux-form": "^7.0.0",
    "redux-modal": "^1.7.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@launchpadlab/babel-preset": "^1.0.0",
    "@launchpadlab/eslint-config": "^2.1.0",
    "@launchpadlab/lp-subsection-generator": "^3.0.1",
    "autoprefixer": "^7.2.5",
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.0",
    "cli-prompt": "^0.6.0",
    "codeclimate-test-reporter": "^0.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "0.28.8",
    "cypress": "^1.0.2",
    "dot-index-webpack-plugin": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "express-enforces-ssl": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "figaro-js": "^1.0.2",
    "file-loader": "^1.1.5",
    "find-root": "^1.1.0",
    "html-webpack-plugin": "2.30.1",
    "interpolate-html-plugin": "^2.0.0",
    "jest": "^22.0.4",
    "js-yaml": "^3.10.0",
    "json-loader": "0.5.7",
    "node-sass": "4.7.2",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-manifest-plugin": "1.3.2",
    "xmp-escape-loader": "^2.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "bourbon"
    ]
  }
}
